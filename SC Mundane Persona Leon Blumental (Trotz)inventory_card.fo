		    <fo:table-body  >
		     <fo:table-row >
		      <fo:table-cell  font-weight="bold">
		       <fo:block >
		 Schnellheilung</fo:block>
		      </fo:table-cell>
		     </fo:table-row>
		     <fo:table-row font-size="10" >
		      <fo:table-cell>
		       <fo:block  >
		Heilung 2</fo:block>
		      </fo:table-cell>
		     </fo:table-row>
		     <fo:table-row>
		      <fo:table-cell>
		       <fo:block  >
		
				</fo:block  >
