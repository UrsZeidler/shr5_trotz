  <inventar xsi:type="shr5:Gegenstand" xmi:id="_qQxKIuRoEe2EtdvPGzsRdw" parentId="_7Hds0AFxEeWgxfrTimxKgg" page="447" verfuegbarkeit="8R" wertValue="1500" beschreibung="Diese Sperrpistole ist eine schnelle und effektive Methode, um mechanische Schl&#xf6;sser zu umgehen. Die Stufe des Automatischen Dietrichs wird zum Limit des Charakters addiert, wenn er ein mechanisches Schloss knackt." name="Autopicker 3" kategorie="Breaking and Entering Gear" stufe="3">
    <localizations xmi:id="_qQxKI-RoEe2EtdvPGzsRdw" local="de" name="Automatischer Dietrich 3"/>
    <srcBook href="platform:/resource/shr5Resource/shr5-1.shr5#f5ec713c-98cd-41f6-a0a4-4a8eaed55b66"/>
